<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2_so"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.prakhar.somethingnew.Main2Activity_so">

    <CheckBox
        android:text="MSO 202a: Complex Analysis"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="70dp"
        android:id="@+id/MSO202" />

    <CheckBox
        android:text="MO 201A: Probability and Statistics"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/MSO203b"
        android:layout_alignParentStart="true"
        android:id="@+id/MSO201" />

    <CheckBox
        android:text="MSO 203b : Differential Equation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/MSO203b"
        android:layout_below="@+id/MSO202"
        android:layout_alignParentStart="true" />

    <CheckBox
        android:text="CSO 201A: Basic Organic Chemstry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/MSO201"
        android:layout_alignParentStart="true"
        android:id="@+id/CSO201" />

    <CheckBox
        android:text="CSO 202A: Atoms ,Molecules and Photons"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/CSO201"
        android:layout_alignParentStart="true"
        android:id="@+id/CSO202" />

    <CheckBox
        android:text="PSO 201A: Quantumm Physics"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/CSO202"
        android:layout_alignParentStart="true"
        android:id="@+id/PSO201" />
</RelativeLayout>
