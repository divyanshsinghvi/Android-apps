<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2_mth"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.prakhar.somethingnew.Main2Activity_mth">
        <CheckBox
            android:text="MTH 301: Real Analysis"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="61dp"
            android:id="@+id/mth301"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />

        <CheckBox
            android:text="MTH 204 :Algebra"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mth301"
            android:layout_alignParentStart="true"
            android:id="@+id/mth204" />

        <CheckBox
            android:text="MTH 302 :Logic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mth204"
            android:layout_alignParentStart="true"
            android:id="@+id/mth302" />

        <CheckBox
            android:text="MTH 421 :ODE"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/mth302"
            android:layout_alignParentStart="true"
            android:id="@+id/mth421" />



</RelativeLayout>
